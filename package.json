{
	"name": "lazulight-one-year-website",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "ts-standard --fix src && tsc --noEmit && prettier --write src/**/*.{css,scss,sass} && stylelint --fix src/**/*.{css,scss,sass}",
		"test": "ts-standard src && tsc --noEmit && prettier --check src/**/*.{css,scss,sass} && stylelint src/**/*.{css,scss,sass}"
	},
	"dependencies": {
		"clsx": "^1.1.1",
		"modern-normalize": "^1.1.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"sass": "^1.49.7",
		"wouter": "^2.8.0-alpha.2"
	},
	"devDependencies": {
		"@types/react": "^17.0.33",
		"@types/react-dom": "^17.0.10",
		"@vitejs/plugin-react": "^1.0.7",
		"prettier": "^2.6.0",
		"stylelint": "^14.7.1",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^3.0.0",
		"ts-standard": "^11.0.0",
		"typescript": "^4.4.4",
		"vite": "^2.7.2",
		"vite-imagetools": "^4.0.3",
		"vite-plugin-checker": "^0.3.4",
		"vite-plugin-compression": "^0.5.1"
	}
}